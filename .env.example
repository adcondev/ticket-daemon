# .env
# Token que deben enviar los clientes WebSocket en el JSON
TICKET_AUTH_TOKEN=mi-token-secreto-123

# Hash Bcrypt codificado en Base64 para el login del Dashboard.
# Ejemplo para password "admin": JDJhJDEwJPH... (debes generar tu propio hash)
TICKET_DASHBOARD_HASH=JDJhJDEwJEx6bzVzLzgzO...

# Entorno: 'local' (verbose logs) o 'remote' (logs filtrados)
BUILD_ENV=local